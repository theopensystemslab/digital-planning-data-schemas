{
  "name": "@opensystemslab/digital-planning-data-schemas",
  "version": "0.0.22",
  "description": "Digital Planning Data schemas",
  "exports": {
    "./types/application": {
      "default": "./dist/types/schemas/application/index.js",
      "types": "./dist/types/schemas/application/index.d.ts"
    },
    "./types/preApplication": {
      "default": "./dist/types/schemas/preApplication/index.js",
      "types": "./dist/types/schemas/preApplication/index.d.ts"
    },
    "./types/prototypeApplication": {
      "default": "./dist/types/schemas/prototypeApplication/index.js",
      "types": "./dist/types/schemas/prototypeApplication/index.d.ts"
    },
    "./types/shared": {
      "default": "./dist/types/shared/index.js",
      "types": "./dist/types/shared/index.d.ts"
    },
    "./schemas": {
      "default": "./dist/schemas/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:types && pnpm build:schema",
    "build:types": "tsc --project tsconfig.build.json",
    "build:examples": "pnpm ts-node scripts/build-json-examples.ts",
    "build:schema": "./scripts/build-schema.sh",
    "lint": "gts lint",
    "check": "tsc --noEmit",
    "fix": "gts fix",
    "test": "pnpm build:schema && vitest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theopensystemslab/digital-planning-data-schemas"
  },
  "keywords": [],
  "author": "Open Systems Lab",
  "license": "MPL2",
  "devDependencies": {
    "@types/geojson": "^7946.0.14",
    "@types/node": "18.16.1",
    "gts": "^6.0.2",
    "husky": "^8.0.0",
    "jsonschema": "^1.4.1",
    "typescript": "~5.5.4",
    "vitest": "^2.1.6"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "rimraf": "^6.0.1",
    "ts-json-schema-generator": "^2.3.0",
    "ts-node": "^10.9.2"
  },
  "packageManager": "pnpm@8.6.6+sha512.6b9d5b209c829afb2d802bec59fe928199ec5df5a6c005b5a47584687da0377bea4060b9f322c52ffa0a070719587d268e9333056457752f5d2dd37a1802b647"
}
